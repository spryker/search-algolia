{
    "name": "spryker/search-algolia",
    "type": "library",
    "description": "SearchAlgolia module",
    "license": "proprietary",
    "require": {
        "php": ">=7.3",
        "spryker/algolia": "*",
        "spryker/kernel": "^3.30.0",
        "spryker/locale": "^3.0.0",
        "spryker/money": "^2.8.0",
        "spryker/search-extension": "^1.1",
        "spryker/store": "^1.0.0",
        "spryker/util-encoding": "^2.1.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/testify": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "test": "vendor/bin/codecept run"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
